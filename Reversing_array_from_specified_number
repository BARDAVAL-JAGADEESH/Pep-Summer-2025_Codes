#include<iostream>
using namespace std;
void reverse(int arr[],int n,int m){
	int start=m+1;
	int end=n-1;
	while(start<=end){
		swap(arr[start],arr[end]);
		start++;
		end--;
	}
}
void printing(int array[],int n){
	for(int i=0;i<n.size();i++){
		cout<<arr[i];
	}
}
int main(){
int arr[5]={1,2,4,5,6};
int pos=3;
reverse(arr,5,pos);
printing(arr,5);

}
