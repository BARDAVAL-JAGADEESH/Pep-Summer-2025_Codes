#include <iostream>
#include <algorithm>
using namespace std;

int circularSubarraySum(int arr[], int n) {
    int res = arr[0];
    
    for(int i = 0; i < n; i++) {
        int currSum = 0;
        
        for(int j = 0; j < n; j++) {
            int idx = (i + j) % n;
            currSum = currSum + arr[idx];
            res = max(res, currSum);
        }
    }
    
    return res;
}

int main() {
    int t;
    cin >> t;
    
    while(t--) {
        int n;
        cin >> n;
        
        int arr[n];
        
        for(int i = 0; i < n; ++i) {
            cin >> arr[i];
        }
        
        cout << circularSubarraySum(arr, n) << endl;
    }

    return 0;
}
