#include <iostream>
using namespace std;

// Function to check if a number is prime
bool isPrime(int num) {
    if (num < 2) return false;
    for (int i = 2; i * i <= num; i++) {
        if (num % i == 0)
            return false;
    }
    return true;
}

int main() {
    int n, count = 0;
    cout << "Enter an integer: ";
    cin >> n;

    int num = n + 1; // Start checking from the next number

    while (count < 5) {
        if (isPrime(num)) {
            cout << num << " ";
            count++;
        }
        num++;
    }

    return 0;
}
