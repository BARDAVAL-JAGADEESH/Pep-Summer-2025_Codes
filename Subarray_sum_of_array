#include<iostream>
using namespace std;
int subarraysum(int arr[],int n){
     int sum=0;
	for(int i=0;i<n;i++){
	//	int sum=0;
		for(int j=i;j<n;j++){
	
	     for (int k = i; k <= j; k++) {
            sum += arr[k];
            }
		}
	}
	
return sum;	

}
int main(){
	int arr[2]={1,2};
	cout<<subarraysum(arr,2);
	return 0;
	
	
}
