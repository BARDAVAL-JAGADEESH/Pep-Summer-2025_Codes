#include <iostream>
#include <vector>

using namespace std;

class Stack {
private:
    vector<int> data;

public:
    // Push element onto stack
    void push(int value) {
        data.push_back(value);
    }

    // Pop element from stack
    void pop() {
        if (!data.empty()) {
            data.pop_back();
        } else {
            cout << "Stack is empty!" << endl;
        }
    }

    // Return top element
    int top() {
        if (!data.empty()) {
            return data.back();
        } else {
            cout << "Stack is empty!" << endl;
            return -1; // or throw an exception
        }
    }

    // Check if stack is empty
    bool isEmpty() {
        return data.empty();
    }

    // Get size of stack
    int size() {
        return data.size();
    }
};

int main() {
    Stack s;

    s.push(10);
    s.push(20);
    s.push(30);

    cout << "Top element: " << s.top() << endl;

    s.pop();
    cout << "Top after pop: " << s.top() << endl;

    cout << "Is empty? " << (s.isEmpty() ? "Yes" : "No") << endl;
    cout << "Size: " << s.size() << endl;

    return 0;
}
